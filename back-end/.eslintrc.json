{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["./node_modules/*"],
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "no-underscore-dangle": "off"
    }
}
